# TuniPark Dashboard Environment Variables
# Copy this file to .env.local for local development
# Set these variables in your Netlify dashboard for production

# App Configuration
VITE_APP_NAME=TuniPark Dashboard
VITE_APP_VERSION=1.0.0
VITE_APP_ENV=production

# API Configuration
VITE_API_URL=https://your-api-endpoint.com
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_DOMAIN=your-auth-domain.com
VITE_JWT_SECRET=your-jwt-secret

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_IOT=true
VITE_ENABLE_AI_CHAT=true

# Third-party Services
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_STRIPE_PUBLIC_KEY=your-stripe-public-key
VITE_SENTRY_DSN=your-sentry-dsn

# WebSocket Configuration
VITE_WS_URL=wss://your-websocket-endpoint.com

# Localization
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=en,ar,fr
